<Window x:Class="tabDonHang.SplashScreen"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:tabDonHang"
        mc:Ignorable="d"
        Title="SplashScreen" Height="450" WindowStyle="None" Width="800" WindowStartupLocation="CenterScreen">
    <Grid>
        <MediaElement x:Name="media" Visibility="Visible" UnloadedBehavior="Manual" MediaEnded="media_MediaEnded" Stretch="Fill"></MediaElement>
    </Grid>
</Window>
